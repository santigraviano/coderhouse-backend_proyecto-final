<table>
  <thead>
    <tr>
      <th>Variable</th>
      <th>Valor</th>
    </tr>
  </thead>
  <tbody>
    <tr>
      <td>Puerto</td>
      <td>{{ config.port }}</td>
    </tr>
    <tr>
      <td>URL de Mongo</td>
      <td>{{ config.database.mongo.url }}</td>
    </tr>
    <tr>
      <td>Tiempo de expiración de la sesión</td>
      <td>{{ config.session.expirationTime }}</td>
    </tr>
    <tr>
      <td>Correo electrónico del administrador</td>
      <td>{{ config.admin.email }}</td>
    </tr>
    <tr>
      <td>Teléfono del administrador</td>
      <td>{{ config.admin.phone }}</td>
    </tr>
  </tbody>
</table>